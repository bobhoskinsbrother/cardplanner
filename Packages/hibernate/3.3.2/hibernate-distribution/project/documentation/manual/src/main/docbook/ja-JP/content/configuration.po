"\n"
"        <!-- cache settings -->\n"
"        <class-cache class=\"org.hibernate.auction.Item\" usage=\"read-write"
"\"/>\n"
"        <class-cache class=\"org.hibernate.auction.Bid\" usage=\"read-only\"/"
">\n"
