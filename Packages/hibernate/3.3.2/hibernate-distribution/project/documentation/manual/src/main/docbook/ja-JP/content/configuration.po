"property>\n"
"        <property name=\"show_sql\">false</property>\n"
"        <property name=\"transaction.factory_class\">\n"
"            org.hibernate.transaction.JTATransactionFactory\n"
"        </property>\n"
"        <property name=\"jta.UserTransaction\">java:comp/UserTransaction</"
"property>\n"
"\n"
"        <!-- mapping files -->\n"
