"    <element column=\"name\" type=\"string\"/>\n"
"</set>\n"
"\n"
"<map name=\"holidays\" order-by=\"hol_date, hol_name\">\n"
"    <key column=\"year_id\"/>\n"
"    <map-key column=\"hol_name\" type=\"string\"/>\n"
