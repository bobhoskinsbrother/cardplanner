"    }\n"
"\n"
"    /**\n"
"     * @hibernate.property\n"
"     *  column=\"SEX\"\n"
"     *  not-null=\"true\"\n"
"     *  update=\"false\"\n"
"     */\n"
"    public char getSex() {\n"
"        return sex;\n"
"    }\n"
"    void setSex(char sex) {\n"
"        this.sex=sex;\n"
"    }\n"
"}]]>"
