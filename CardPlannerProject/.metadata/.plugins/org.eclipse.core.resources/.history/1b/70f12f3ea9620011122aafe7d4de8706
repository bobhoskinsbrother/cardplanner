package uk.co.itstherules.cardplanner.model;

import javax.persistence.Entity;

import uk.co.itstherules.cardplanner.model.CachedInstance.Identities;
import uk.co.itstherules.yawf.model.TemplateModel;
import uk.co.itstherules.yawf.model.persistence.ObjectCache;

@Entity  
public final class CardPlannerPageModel extends PageModel {
	
	public CardPlannerPageModel() {}
	
    public CardPlannerPageModel defaultSetup(ObjectCache objectCache) {
		setTemplate(SpecialInstances.<TemplateModel>retrieve(objectCache, Identities.CARDPLANNER_TEMPLATE));
		setParent(SpecialInstances.<CardPlannerPageModel>retrieve(objectCache, Identities.NULL_PAGE));
		super.defaultSetup(objectCache);
		return this;
	}
}